protected void doPost(HttpServletRequest request, HttpServletResponse response) throws ServletException, IOException {
        //1.1 ì „ì†¡ëœ ê°’ì„ utf-8 ì…‹íŒ…í•˜ê¸°
        request.setCharacterEncoding("UTF-8");
        //1.1 ì •ë³´ê°€ì ¸ì˜¤ê¸°
        String writer = request.getParameter("writer"); 
        String memo = request.getParameter("memo");

        //2. visit í…Œì´ë¸”ì— ì €ì¥í•œë‹¤(í”„ë¡œí† ì½œ: ì•½ì†)
        Connection con = null;
        PreparedStatement pstmt = null;     //ì˜¤ë¼í´ì—ì„œ ì‘ì—…í•  ì¿¼ë¦¬ë¬¸ ì‚¬ìš©í• ê²Œ í•˜ëŠ” ëª…ë ¹ë¬¸
        boolean successFlag = false; 
        String url = "jdbc:oracle:thin:@127.0.0.1:1521/xe";
        String VISIT_INSERT = "INSERT INTO VISIT VALUES(VISIT_seq.NEXTVAL, ?, ?, SYSDATE)";
        try {
            Class.forName("oracle.jdbc.driver.OracleDriver");
            con = DriverManager.getConnection(url, "webuser", "123456");
            pstmt = con.prepareStatement(VISIT_INSERT);
            pstmt.setString(1, writer);
            pstmt.setString(2, memo); 
            int count = pstmt.executeUpdate();
            successFlag = (count != 0)?(true):(false); 

        } catch (ClassNotFoundException e) {
            System.out.println(e.toString());
        } catch (SQLException e) {
            System.out.println(e.toString());
        } finally {
            if (con != null) {
                try {
                    con.close();
                } catch (SQLException e) {
                    System.out.println(e.toString());
                }
            }
            if (pstmt != null) {
                try {
                    pstmt.close();
                } catch (SQLException e) {
                    System.out.println(e.toString());
                }
            }
        }

        //3. í™”ë©´
        if(successFlag == true) {
            System.out.println("ì…ë ¥ì„±ê³µ");
//            response.sendRedirect("visitList");

        }else {
            System.out.println("ì…ë ¥ì‹¤íŒ¨");

        }
    }
