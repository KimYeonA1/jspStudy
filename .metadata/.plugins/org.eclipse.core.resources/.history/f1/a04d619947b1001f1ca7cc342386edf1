public String toString() {

        SimpleDateFormat sdf = new SimpleDateFormat("yyyy/mm/dd");
        String rd = sdf.format(releaseDate);
        return String.format("%-10d%-20s%-10d%-15s%-30s"
                , movieId
                , rd
                , duration
                , genre
        , title);
    }
