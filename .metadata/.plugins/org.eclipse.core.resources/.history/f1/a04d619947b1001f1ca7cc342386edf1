CREATE table "VISIT" (
    "NO"         NUMBER(5,0) NOT NULL,
    "WRITER"     VARCHAR2(20) NOT NULL,
    "MEMO"     VARCHAR2(4000) NOT NULL,
    "REGDATE"   DATE NOT NULL,
    constraint "VISIT_PK" primary key ("NO")
);
